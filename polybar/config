[module/mem]
type = custom/script
interval = 2
exec = free -m | sed -n 's/^Mem:\s\+[0-9]\+\s\+\([0-9]\+\)\s.\+/\1/p'
format = <label>

label = %output% MB
label-padding = 0

format-prefix = "﬙ "
format-prefix-foreground = #fc620a
format-prefix-background =${colors.background}
label-foreground = #fcaf0a
label-background =${colors.background}



