line-size = 0
padding = 0

border-size = 01
border-top-size = 0
border-bottom-size = 1
border-left-size = 0
border-right-size =0

padding-left = 1
padding-right = 1
padding-top = 0
module-margin-right = 0
module-margin-left = 0

wm-restack = bspwm

separator =
separator-padding = 0


