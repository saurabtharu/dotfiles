label-volume = %percentage:2%%

format-muted = <label-muted>
format-muted-padding = 2
label-muted =   0%
label-muted-foreground = ${colors.danger}

ramp-volume-0 = 
ramp-volume-1 = 
ramp-volume-2 = 





